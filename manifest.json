{
    "manifest_version": 3,
    "name": "Meow",
    "version": "1.0",
    "description": "See if it's raining cats & cats",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_scripts": [
        {
            "css": ["styles.css"],
            "js": ["main.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "icons": {
        "16": "cat16.png",
        "32": "cat32.png",
        "48": "cat48.png",
        "128": "cat128.png"
    },
    "action": {
        "default_icon": {
            "16": "cat16.png",
            "32": "cat32.png"
        }
    }
}
